@model EasyFreight.ViewModel.CompanySetupVm
<div class="content-wrapper">
    <div class="col-xs-12 printfooteraddress">
        <div class="col-xs-5">
            <div class="col-xs-12">
                @Model.CompanyAddressEn
            </div>
            <div class="col-xs-12" style="direction: ltr;padding-right: 0px!important;float:left">  
               <div class="col-xs-12 col-sm-4 col-md-4 col-lg-3">Tel. Fax :</div>
                <div class="col-xs-12 col-sm-8 col-md-8 col-lg-9 ">
                    002 03  @Model.PhoneNumber1 - @Model.PhoneNumber2 - @Model.PhoneNumber3
                </div>
                
            </div>
        </div>
        <div class="col-xs-1">
            <img src="@Url.Content("~/images/FooterLetLogo.png")" height="50" />
        </div>
        <div class="col-xs-1">
            <img src="@Url.Content("~/images/FooterRightLogo.png")" height="50" />
        </div>
        <div class="col-xs-5" style="text-align: right;direction: rtl; ">
            <div class="col-xs-12">
                @Model.CompanyAddressAr
            </div>
            <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12" style="direction: rtl;padding-right: 0px!important;" >
                <div class="col-xs-12 col-sm-4 col-md-4 col-lg-3 pull-right" style=" padding-right: 0px!important;" >تليفون و فاكس</div>
                <div class="col-xs-12 col-sm-8 col-md-8 col-lg-9" style="direction: ltr">+2 03 @Model.PhoneNumber1 - @Model.PhoneNumber2 - @Model.PhoneNumber3
                </div>

            </div>
        </div>
    </div>

    <div class="col-xs-12 printfooteremail" style="direction: ltr;padding-right: 0px!important;float:left" >
        Email : @Model.Email
    </div>
</div>