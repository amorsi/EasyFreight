                    <span class="text-dark"> </span>
                </div>
                <label class="col-md-5 col-xs-5 control-label">: بلد المنشأ  </label>

            </div>

            <div class="col-sm-6 col-xs-6 form-group">
                <div class="col-md-7 col-xs-7 text-right">
                    @Model.ToPort
                </div>
                <label class="col-md-5 col-xs-5 control-label">: ميناء الوصول</label>
            </div>
       
             </div>

