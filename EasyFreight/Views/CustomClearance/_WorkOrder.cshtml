                    @ViewBag.ContainersSummary
                </div>
                <label class="col-md-5 col-xs-5 control-label">: نوع الحاويات</label>
            </div>
       
            <div class="col-sm-6 col-xs-6 form-group text-left">

                <div class="col-md-7 col-xs-7 text-right">
                    @(((DateTime)Model.DateOfDeparture).ToString("dd/MM/yyyy"))
                </div>
                <label class="col-md-3 col-xs-3 control-label">: تاريخ الرحلة</label>

            </div>

            <div class="col-sm-6 col-xs-6 form-group">
