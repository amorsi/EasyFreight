                    @Model.CarrierNameEn
                </div>
                <label class="col-md-5 col-xs-5 control-label">: الخط الملاحي</label>
            </div>
            
           
             

          </div>

        <div  @(Model.OrderFrom == 1 ? "style=display:none;" : "style=display:block;")>

            <div class="col-sm-6 col-xs-6 form-group">

                <div class="col-md-7 col-xs-7 text-right">
                    <span class="text-dark">@(Model.ArriveDate != null ? Model.ArriveDate.Value.ToString("dd/MM/yyyy") : "")</span>
                </div>
                <label class="col-md-5 col-xs-5 control-label">: تاريخ الوصول </label>

            </div>

            <div class="col-sm-6 col-xs-6 form-group">
