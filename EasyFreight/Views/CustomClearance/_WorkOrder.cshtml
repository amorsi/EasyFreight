@model EasyFreight.Models.CustomClearanceView

      
    <div class="row form-bordered" >
          
           <div class="col-sm-6 col-xs-6 form-group"> </div> 

           <div class="col-sm-6 col-xs-6 form-group">
            
            <div class="col-md-7 col-xs-7 text-right">
                <span class="text-dark"> </span>
            </div>
            <label class="col-md-5 col-xs-5 control-label"> : التاريخ</label>

           </div> 

        <div class="col-sm-6 col-xs-6 form-group"> </div> 

           <div class="col-sm-6 col-xs-6 form-group">
            
            <div class="col-md-7 col-xs-7 text-right">
              @(Model.OrderFrom == 1 ? Model.ShipperNameEn : Model.ConsigneeNameEn )
            </div>
            <label class="col-md-5 col-xs-5 control-label">  : اسم العميل</label>

           </div>



        <div @(Model.OrderFrom == 1 ? "style=display:block;" : "style=display:none;")>
            <div class="col-sm-6 col-xs-6 form-group text-left">

                <div class="col-md-7 col-xs-7 text-right ">
                    <span class="text-dark">@(((DateTime)Model.DateOfDeparture).ToString("dd/MM/yyyy"))  </span>
                </div>
                <label class="col-md-3 col-xs-3 control-label">: ميعاد التحميل  </label>

            </div>

            <div class="col-sm-6 col-xs-6 form-group">
